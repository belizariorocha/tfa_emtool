<div class="page-header">
  <h3>Warden #<%= @warden.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Role</dt>
      <dd><%= @warden.role %></dd>

      <dt>Acknowledged</dt>
      <dd><%= @warden.acknowledged %></dd>

      <dt>Office</dt>
      <dd><%= @warden.office_id %></dd>

      <dt>Staff member</dt>
      <dd><%= @warden.staff_member_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/wardens" class="btn btn-primary">
        Back
      </a>
      <a href="/wardens/<%= @warden.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_warden/<%= @warden.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
